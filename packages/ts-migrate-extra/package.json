{
  "name": "ts-migrate-extra",
  "version": "0.0.4",
  "description": "A tool for migrating frontend application from JavaScript to TypeScript",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "ts-migrate-extra": "build/cli.js",
    "ts-migrate-extra-full": "bin/ts-migrate-extra-full.sh"
  },
  "keywords": [
    "ts-migrate",
    "typescript",
    "migration",
    "codemod"
  ],
  "files": [
    "build",
    "bin",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/euclidyr/ts-migrate-extra.git",
    "directory": "packages/ts-migrate-extra"
  },
  "bugs": {
    "url": "https://github.com/euclidyr/ts-migrate-extra/issues"
  },
  "scripts": {
    "test": "jest --config jest-config.json",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx --ignore-path ../../.eslintignore",
    "build": "npm run clean && tsc -p tsconfig.json",
    "prepublish": "yarn build",
    "clean": "rm -rf build"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "create-jest-runner": "^0.5.3",
    "json5": "^2.1.1",
    "json5-writer": "^0.1.8",
    "ts-migrate-plugins": "^0.1.35",
    "ts-migrate-server": "^0.1.33",
    "updatable-log": "^0.2.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.16",
    "@types/strip-comments": "^2.0.1",
    "glob": "^7.1.6",
    "jest": "26.6.3",
    "react": "^16.12.0"
  },
  "gitHead": "7de041cf23f38136ba2ca27bced2eca291cd3ee6",
  "peerDependencies": {
    "typescript": "^5.1.3"
  }
}
